@article{Mauricio:2009a,
abstract = {This paper presents a new method to enhance the participation of variable-speed wind energy conversion systems (WECS) in existing frequency regulation mechanisms. The proposed approach, based on a modified inertial control scheme, takes advantage of the fast response capability associated with electronically-controlled WECS, allowing the kinetic energy stored by rotational masses to be partly and transiently released in order to provide earlier frequency support. An additional improvement is achieved by communicating the WECS response to conventional generators so that these can eventually take care of the full load imbalance. Several simulations using a two-area test system are performed to demonstrate the benefits of the proposed scheme.},
author = {Mauricio, Juan Manuel J.M. and Marano, Alejandro and Gomez-Exposito, A. and {Martinez Ramos}, J.L. and G\'{o}mez-Exp\'{o}sito, Antonio and Ramos, J. L M},
doi = {10.1109/TPWRS.2008.2009398},
issn = {0885-8950},
journal = {IEEE Transactions on Power Systems},
keywords = {Frequency regulation,Variable-speed generators,Wind energy conversion,electronically-controlled WECS,frequency control,frequency regulation contribution,full load imbalance,kinetic energy,modified inertial control scheme,power conversion,power system control,two-area test system,variable-speed generators,variable-speed wind energy conversion systems,wind energy conversion,wind power},
month = feb,
number = {1},
pages = {173--180},
shorttitle = {Power Systems, IEEE Transactions on},
title = {{Frequency regulation contribution through variable-speed wind energy conversion systems}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=4749370},
volume = {24},
year = {2009}
}
