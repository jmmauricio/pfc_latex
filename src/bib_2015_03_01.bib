@article{Eriksson:11,
abstract = {The contribution of this paper is the application of subspace system identification techniques, to derive a low-order black-box state-space model of a power system with many controllable devices using global signals. This model is a multiinput, multioutput open system model describing the power oscillatory behavior of the power system. The input signals are the controllable setpoints of the controllable devices, the output signals are the speed of selected generators measured by a wide-area measurement system. This paper describes how to achieve and preprocess the data to use subspace techniques to estimate and validate to finally assign an accurate model. This new approach can be used directly to design a central coordinating controller for all of the relevant controllable devices, with the aim to increase the damping of the modes in the system. Previously presented methods use local measurements or output signals dependent on the actual operational point. The benefit of the presented method is that the used output signals are independent of the system state. This makes it possible to use state-feedback control to combine the controllable devices to coordinately damp the modes. The presented method is applied in the CIGREÌ Nordic 32-bus system including two HVDC links. The case study demonstrates that accurate low-order state-space models can be estimated and validated by using the described method to accurately model the system's power oscillatory behavior.},
author = {Eriksson, Robert and Soder, Lennart},
doi = {10.1109/TPWRD.2010.2094628},
file = {:home/jmmauricio-m/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Eriksson, Soder - 2011 - Wide-Area Measurement System-Based Subspace Identification for Obtaining Linear Models to Centrally Coordinate.pdf:pdf},
issn = {0885-8977},
journal = {IEEE Transactions on Power Delivery},
keywords = {CIGREÌ Nordic 32-bus system,Flexible ac transmission system (FACTS) devices,HVDC,HVDC links,N4SID,central coordinating controller design,centralised control,centrally coordinate controllable devices,feedback,generators,global signals,interarea oscillations,linear models,low-order black-box state-space model,modal analysis,multiinput multioutput open system model,nordic 32-bus system,phasor measurement unit (PMU),power system,power system control,power system identification,power system measurement,small-signal analysis,state-feedback control,state-space methods,subspace system identification,subspace system identification techniques,system damping,system power oscillatory behavior,wide-area measurement system,wide-area measurement systems (WAMS)},
month = apr,
number = {2},
pages = {988--997},
shorttitle = {Power Delivery, IEEE Transactions on},
title = {{Wide-Area Measurement System-Based Subspace Identification for Obtaining Linear Models to Centrally Coordinate Controllable Devices}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=5680992},
volume = {26},
year = {2011}
}
