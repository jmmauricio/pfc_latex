@misc{MaiHuongNguyen2012,
abstract = {Several studies on transient stability analysis of power systems with hybrid Multi-terminal HVDC (MTDC) have been conducted, but very few have focused on small signal stability and designing oscillation damping controller. As a hybrid MTDC system has its distinctive characteristics due to interaction between Line Commutated Converter (LCC) and Voltage Source Converter (VSC) converters, with nearby wind farm, SVCs and other electronic equipment, impacts of its components on the electromechanical modes of the system need to be investigated thoroughly. In this paper, the Simplified Southern Eastern (SE) Australian system has been modified and extended to explore network stability impacts of the hybrid MTDC links. System identification is implemented to determine the linearized model and the electromechanical oscillations of the power system.},
author = {{Mai Huong Nguyen} and Saha, T.K. and Eghbal, M.},
