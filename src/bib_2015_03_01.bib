mendeley-tags = {statcom},
month = mar,
number = {1},
pages = {226--233},
shorttitle = {Energy Conversion, IEEE Transactions on},
title = {{STATCOM Impact Study on the Integration of a Large Wind Farm into a Weak Loop Power System}},
