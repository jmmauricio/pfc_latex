isbn = {978-1-4244-6549-1},
issn = {1944-9925},
keywords = {Continental Europe power system,Eigenvalue Analysis,Inter-area Oscillations,Power System Stabilizer,Static Stability,Turkish power system,Wide Area Monitoring,current oscillation,damping,inter-area electromechanical oscillation,interconnected electric power system,low frequency oscillation,oscillations,power exchange,power system interconnection,power system measurement,power system monitoring,power system stability,power system stabilizer,synchronous interconnected system,voltage oscillation},
month = jul,
pages = {1--7},
